@model EDDProyecto.Models.Usuario
@{
    ViewBag.Title = "Login";
}

<h1>Login</h1>



@using (@Html.BeginForm("Log", "Usuario", FormMethod.Post))
{
    <p>Usuario:</p>
    <p>@Html.TextBoxFor(Model => Model.Username)</p>
    @:&nbsp;
    <p>Contraseña:</p>
    <p>@Html.TextBoxFor(Model => Model.Password)</p>

    <input type="Submit" name="Ingresar" value="Ingresar" />
}


